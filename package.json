{
  "name": "vue3-page-generator",
  "version": "0.0.1",
  "description": "Vue3.0 动态页面设计生成器",
  "main": "index.js",
  "repository": "https://github.com/xfy520/vue3-page-generator.git",
  "author": "xfy520 <longzinziyan@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vite --mode development",
    "build": "vuedx-typecheck . && vite build --mode production",
    "svg": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "br": "webpack-bundle-analyzer --port 8123 dist/stats.json",
    "hs": "http-server ./dist -p 8888 -c-1"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "axios": "^0.20.0-0",
    "element-plus": "^1.2.0-beta.3",
    "throttle-debounce": "^3.0.1",
    "vue": "^3.2.22",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.12",
    "vuedraggable": "4.1.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.22",
    "@vuedx/typecheck": "^0.7.4",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "autoprefixer": "^10.3.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-vue": "^7.16.0",
    "http-server": "^13.0.1",
    "less": "^4.1.1",
    "postcss-import": "^14.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "svgo": "^2.4.0",
    "typescript": "^4.3.5",
    "vite": "^2.6.14",
    "vite-plugin-compression": "^0.3.3",
    "vite-plugin-html": "^2.1.0",
    "vite-plugin-style-import": "^1.1.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vue-eslint-parser": "^7.10.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "bugs": {
    "url": "https://github.com/xfy520/vue3-page-generator/issues"
  },
  "homepage": "https://github.com/xfy520/vue3-page-generator",
  "changelog": {
    "bugsUrl": "https://github.com/xfy520/vue3-page-generator",
    "emojis": true,
    "authorName": false,
    "authorEmail": false
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
